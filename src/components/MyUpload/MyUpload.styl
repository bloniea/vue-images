:deep(.upload-dialog)
    // width
    width 70%

    @media (max-width 767px)
        width 90%

    @media (max-width 557px)
        width 96%

    .tabs
        .image
            height 300px
            margin-top 10px

            .el-image
                width 100%
                height 100%

    .upload-success
        .upload-success-img
            margin-top 10px

            .success-img
                height 50vw

                .el-image
                    width 100%
                    height 100%

        .url-input
            display flex
            margin 10px

            .el-input
                margin-right 10px